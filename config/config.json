{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "crud-operation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/crudOperation/CrudOperationWebPart.js",
          "manifest": "./src/webparts/crudOperation/CrudOperationWebPart.manifest.json"
        }
      ]
    },
    "fetch-large-data-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fetchLargeData/FetchLargeDataWebPart.js",
          "manifest": "./src/webparts/fetchLargeData/FetchLargeDataWebPart.manifest.json"
        }
      ]
    },
    "paginated-table-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/paginatedTable/PaginatedTableWebPart.js",
          "manifest": "./src/webparts/paginatedTable/PaginatedTableWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CrudOperationWebPartStrings": "lib/webparts/crudOperation/loc/{locale}.js",
    "FetchLargeDataWebPartStrings": "lib/webparts/fetchLargeData/loc/{locale}.js",
    "PaginatedTableWebPartStrings": "lib/webparts/paginatedTable/loc/{locale}.js"
  }
}
